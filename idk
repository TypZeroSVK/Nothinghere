 local Toggle = Tab:CreateToggle({
    Name = "Always get AirDrop",
    CurrentValue = false,
    Flag = "Toggle1",
    Callback = function(Value)
        while Value == true do 
          wait(0.3)
          local hrp = game.Players.LocalPlayer.Character.HumanoidRootPart
          local maina = game.Workspace.Airdrops.Airdrop.Main
          if maina then
            local bhrp = Instance.new("Part")
            bhrp.Anchored = true
            bhrp.CFrame = hrp.CFrame
            hrp.CFrame = maina.CFrame
            maina.ProximityPrompt.Enabled = true
            game:GetService("Players").LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame = maina.CFrame
            fireproximityprompt(maina.ProximityPrompt)
            bhrp.CFrame = hrp.CFrame
            bhrp:Destroy()
          end
        end
       
    end,
 })


